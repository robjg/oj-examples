<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<oddjob id="this">
    <job>
        <sequential>
            <jobs>
                <script exportAll="true" id="script" name="Define Function"><![CDATA[function add(x, y) {
	return x + y
}

function multiply(x, y) {
	return x * y
}
]]></script>
                <echo><![CDATA[2 + 3 is #{add(2, 3)}, 2 * 3 is #{multiply(2, 3)}.]]></echo>
            </jobs>
        </sequential>
    </job>
</oddjob>
